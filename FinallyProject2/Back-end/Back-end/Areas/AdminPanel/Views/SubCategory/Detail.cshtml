@model SubCategoryListVM

<div class="container">
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Sub category</h4>
                <div class="table-responsive pt-3">
                    <table class="table table-bordered my-3">
                        <thead>
                            <tr>
                                <th>
                                    Category
                                </th>
                                <th>
                                    Sub Category
                                </th>
                            </tr>
                        </thead>
                        <tbody>

                            <tr>
                                <td style="font-size: 25px;">
                                    @Html.Raw(Model.Category)
                                </td>
                                <td style="font-size: 25px;">
                                    @Html.Raw(Model.SubCategory)
                                </td>
                            </tr>
                        </tbody>
                    </table>
                    <a asp-controller="SubCategory" asp-action="Index" style="color: white;" class="btn btn-danger my-3">Back</a>
                </div>
            </div>
        </div>
    </div>
</div>